<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<constant name="struts.custom.i18n.resources" value="csdn"></constant>
	
	<package name="myi18n" namespace="/" extends="struts-default">
	<!-- method默认是execute -->
		<action name="myi18n" class="com.company.action.Myi18nAction">
			<!-- result 默认name=success -->
			<result>/demo_i18n.jsp</result>
		</action>
	</package>
</struts>
